     "output_type": "execute_result"
    }
   ],
   "source": [
    "import promptulate as pne\n",
    "\n",
    "model_config = {\"temperature\":\"0.1\"}\n",
    "llm = pne.LLMFactory.build(\"zhipu\",model_name=\"glm-4\",model_config=model_config)\n",
    "llm(\"what can you do\")"
